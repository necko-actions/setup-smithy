  "license": "MIT",
  "packageManager": "yarn@4.1.0",
  "devDependencies": {
    "@actions/cache": "^3.2.4",
    "@actions/core": "^1.10.1",
    "@actions/glob": "^0.4.0",
    "@actions/tool-cache": "^2.0.1",
    "@biomejs/biome": "^1.5.3",
    "@tsconfig/recommended": "^1.0.3",
    "@types/node": "^20.11.24",
